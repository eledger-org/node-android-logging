{
  "name": "node-android-logging",
  "version": "1.0.2",
  "description": "A node.js logging library intended to replicate the Android logging format",
  "main": "index.js",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "dependencies": {
    "circular-json": "^0.3.0",
    "node-extra-funcs": ">=1.0.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^2.13.0",
    "mocha": "^2.5.3"
  },
  "scripts": {
    "test": "./bin/run_tests.sh",
    "docgen": "yuidoc -o gen ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eledger-org/node-android-logging.git"
  },
  "author": "Henry Brown",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eledger-org/node-android-logging/issues"
  },
  "homepage": "https://github.com/eledger-org/node-android-logging#readme",
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "comma-dangle": "warn",
      "semi": "error",
      "curly": "error",
      "quotes": [
        "error",
        "double"
      ],
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ]
    }
  }
}
